  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "sudo docker compose --file infra/compose.yaml up --detach && next dev",
    "service:up": "docker compose --file infra/compose.yaml up --detach",
    "service:stop": "docker compose --file infra/compose.yaml stop",
    "service:down": "docker compose --file infra/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "build": "next build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m infra/migration create",
    "migration:up": "node-pg-migrate -m infra/migration --envPath .env.development up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.0.3"
  }
}
